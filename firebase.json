{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "**",
        "apphosting": {
          "backendId": "{{apphosting-backend-id}}",
          "region": "us-east1"
        }
      }
    ]
  }
}
