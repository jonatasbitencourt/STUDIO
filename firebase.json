{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "{{apphosting-backend-id}}",
          "region": "us-east1"
        }
      }
    ]
  }
}
